<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.weact.pastryresearchrecyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_customlauncher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_customlauncher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PastryResearchRecyclerView">
        <activity android:name=".FoodScreen"></activity>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>